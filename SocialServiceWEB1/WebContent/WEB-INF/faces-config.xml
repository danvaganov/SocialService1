<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.socialservice.jsf.bean.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mainContentBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.socialservice.jsf.bean.MainContentBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>personBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.socialservice.jsf.bean.PersonBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>loginBean</property-name>
			<property-class>ru.bisoft.socialservice.jsf.bean.LoginBean</property-class>
			<value>#{loginBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>personEducationBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.socialservice.jsf.bean.PersonEducationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>personBean</property-name>
			<property-class>ru.bisoft.socialservice.jsf.bean.PersonBean</property-class>
			<value>#{personBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>educationBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.socialservice.jsf.bean.EducationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>educationConverter</managed-bean-name>
		<managed-bean-class>ru.bisoft.socialservice.jsf.converter.EducationConverter</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>XMLDataBean</managed-bean-name>
		<managed-bean-class>ru.bisoft.socialservice.jsf.bean.XMLDataBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

</faces-config>
